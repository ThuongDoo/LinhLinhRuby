<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>

<% if current_user %>
  <h1><%= link_to "Log out", session_path, data: {
  turbo_method: :delete,
  turbo_confirm: "Are you sure?"
} %></h1>
<% else %>
  <h1><%= link_to "Login", new_session_path %></h1>
  <h1><%= link_to "Sign up", new_user_path %></h1>

<% end %>

  
  
